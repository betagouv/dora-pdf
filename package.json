{
  "name": "dora-pdf",
  "version": "0.0.1",
  "engines": {
    "node": "16",
    "npm": "7"
  },
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "lint": "stylelint \"**/*.{css,html,svelte}\" && prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. . && eslint --ignore-path .gitignore --fix .",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "puppeteer": "^10.2.0"
  },
  "devDependencies": {
    "@sentry/node": "^6.12.0",
    "@sentry/tracing": "^6.12.0",
    "eslint": "^7.32.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write --plugin-search-dir=.",
      "eslint --ignore-path .gitignore --fix"
    ]
  }
}
